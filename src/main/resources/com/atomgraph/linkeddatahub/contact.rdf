    </rdf:Description>

    <rdf:Description rdf:about="http://www.w3.org/2000/10/swap/pim/contact#title">
        <domain rdf:resource="http://www.w3.org/2000/10/swap/pim/contact#Person"/>
    </rdf:Description>

