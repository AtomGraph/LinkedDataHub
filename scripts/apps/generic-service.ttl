@prefix ns:	<ns#> .
@prefix dct:	<http://purl.org/dc/terms/> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@prefix dh:	<https://www.w3.org/ns/ldt/document-hierarchy/domain#> .
@prefix sd:	<http://www.w3.org/ns/sparql-service-description#> .
@prefix a:	<http://atomgraph.com/ns/core#> .
@prefix srv:	<http://jena.hpl.hp.com/Service#> .

_:service a ns:GenericService ;
  dct:title "${title}" ;
  sd:endpoint <${endpoint}> ;
  sd:supportedLanguage sd:SPARQL11Query, sd:SPARQL11Update ;
  a:graphStore <${graph_store}> ;
  srv:queryAuthUser "${service_user}" ;
  srv:queryAuthPwd "${service_password}" ;
  foaf:isPrimaryTopicOf _:item .

_:item a ns:ServiceItem ;
  dct:title "${title}" ;
  dh:slug "${slug}" ;
  foaf:primaryTopic _:service .