@prefix ns:	<ns#> .
@prefix ldt:	<https://www.w3.org/ns/ldt#> .
@prefix dct:	<http://purl.org/dc/terms/> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@prefix dh:	<https://www.w3.org/ns/ldt/document-hierarchy/domain#> .
@prefix lapp:	<http://linkeddatahub.com/ns/apps/domain#> .

_:app a ns:EndUserApplication ;
  dct:title "${title}" ;
  lapp:adminApplication <${admin_app}> ;
  ldt:base <${app_base}> ;
  ldt:ontology <${app_base}ns#> ;
  ldt:service <${service}> ;
  foaf:isPrimaryTopicOf _:item .

_:item a ns:EndUserApplicationItem ;
  dct:title "${title}" ;
  dh:slug "${slug}" ;
  foaf:primaryTopic _:app .