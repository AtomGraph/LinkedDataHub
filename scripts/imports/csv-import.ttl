@prefix ns:	<ns#> .
@prefix apl:	<http://atomgraph.com/ns/platform/domain#> .
@prefix dct:	<http://purl.org/dc/terms/> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@prefix dh:	<https://www.w3.org/ns/ldt/document-hierarchy/domain#> .
@prefix dydra:	<http://dydra.com/ns#> .
@prefix srv:	<http://jena.hpl.hp.com/Service#> .
@prefix spin:	<http://spinrdf.org/spin#> .

_:import a ns:CSVImport ;
  dct:title "${title}" ;
  spin:query <${query}> ;
  apl:action <${action}> ;
  apl:file <${file}> ;
  apl:delimiter "," ;
  foaf:isPrimaryTopicOf _:item .

_:item a ns:ImportItem ;
  dct:title "${title}" ;
  dh:slug "${slug}" ;
  foaf:primaryTopic _:import .