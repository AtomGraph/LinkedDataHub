@prefix adm:	<ns#> .
@prefix nsd:	<../ns/domain#> .
@prefix nst:	<../ns/templates#> .
@prefix def:	<../ns/default#> .
@prefix rdfs:	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:	<http://www.w3.org/2002/07/owl#> .
@prefix ldt:	<https://www.w3.org/ns/ldt#> .
@prefix spin:	<http://spinrdf.org/spin#> .
@prefix sioc:	<http://rdfs.org/sioc/ns#> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .
@prefix dct:	<http://purl.org/dc/terms/> .
@prefix msg:    <https://w3id.org/atomgraph/linkeddatahub/modules/messages#> .

nsd: owl:imports msg: .

# DOMAIN

# message

<model/classes/messages/message/> a adm:ClassItem ;
    sioc:has_container <model/classes/messages/> ;
    dct:title "Message" ;
    foaf:primaryTopic nsd:Message .

nsd:Message a adm:Class ;
    foaf:isPrimaryTopicOf <model/classes/messages/message/> ;
    rdfs:subClassOf msg:Message, nsd:TopicOfMessageItem ;
    rdfs:label "Message" ;
    rdfs:isDefinedBy nsd: .

# message item

<model/classes/messages/message-item/> a adm:ClassItem ;
    sioc:has_container <model/classes/messages/> ;
    dct:title "Message item" ;
    foaf:primaryTopic nsd:MessageItem .

nsd:MessageItem a adm:Class ;
    foaf:isPrimaryTopicOf <model/classes/messages/message-item/> ;
    rdfs:subClassOf def:Item, nsd:ItemOfMessagesContainer ;
    rdfs:label "Message item" ;
    rdfs:isDefinedBy nsd: .

# topic of message item

<model/restrictions/messages/topic-of-message-item/> a adm:RestrictionItem ;
    sioc:has_container <model/restrictions/messages/> ;
    dct:title "Topic of message item" ;
    foaf:primaryTopic nsd:TopicOfMessageItem .

nsd:TopicOfMessageItem a adm:Restriction ;
    foaf:isPrimaryTopicOf <model/restrictions/messages/topic-of-message-item/> ;
    owl:onProperty foaf:isPrimaryTopicOf ;
    owl:allValuesFrom nsd:MessageItem ;
    rdfs:label "Topic of message item" ;
    rdfs:isDefinedBy nsd: .

# has messages container

<model/restrictions/messages/messages-container/> a adm:RestrictionItem ;
    sioc:has_container <model/restrictions/messages/> ;
    dct:title "Has messages container" ;
    foaf:primaryTopic nsd:ItemOfMessagesContainer .

nsd:ItemOfMessagesContainer a adm:Restriction ;
    foaf:isPrimaryTopicOf <model/restrictions/messages/messages-container/> ;
    owl:onProperty sioc:has_container ;
    owl:hasValue <../messages/> ;
    rdfs:label "Has messages container" ;
    rdfs:isDefinedBy nsd: .

# SITEMAP

# message

<sitemap/templates/messages/container/> a adm:TemplateItem ;
    sioc:has_container <sitemap/templates/messages/> ;
    dct:title "Message container" ;
    foaf:primaryTopic nst:MessageContainer .

nst:MessageContainer a adm:Template ;
    foaf:isPrimaryTopicOf <sitemap/templates/messages/container/> ;
    ldt:loadClass <java:com.atomgraph.linkeddatahub.resource.message.Container> ;
    rdfs:label "Message container" ;
    rdfs:isDefinedBy nst: .
