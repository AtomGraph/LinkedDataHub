<#content2> a apl:Content ;
    rdf:first "<div xmlns=\"http://www.w3.org/1999/xhtml\"><p>second</p></div>"^^<http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral> ;
    rdf:rest rdf:nil .

