@prefix sioc:	<http://rdfs.org/sioc/ns#> .
@prefix foaf:	<http://xmlns.com/foaf/0.1/> .

<>
{

    <> a def:Root ;
        dct:title "Root" ;
        dct:description "Root container" ;
        ldh:content <#select-children> .

    <#select-children> a ldh:Content ;
        rdf:first ldh:SelectChildren ;
        rdf:rest rdf:nil .

}

